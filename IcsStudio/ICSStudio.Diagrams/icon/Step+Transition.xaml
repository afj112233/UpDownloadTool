<UserControl x:Class="ICSStudio.Diagrams.icon.Step_Transition"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ICSStudio.Diagrams.icon"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
            <Border Width="40" Height="30" Background="#00000000">
                <StackPanel Orientation="Vertical">
                    <Rectangle Width="20" Height="10" Stroke="Black" StrokeThickness="1" Margin="5"></Rectangle>
                    <Line X1="12" X2="28" Stroke="Black" StrokeThickness="1" />
                    <Line X1="20" X2="20" Y1="-5" Y2="5" Stroke="Black" StrokeThickness="1"></Line>
            </StackPanel>
            </Border>
    </Grid>
</UserControl>

<UserControl x:Class="ICSStudio.Diagrams.icon.Stop"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ICSStudio.Diagrams.icon"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
            <Border Background="#0000" Width="40" Height="30">
                <Grid>
                    <Ellipse Width="20" Height="20" Stroke="Black" VerticalAlignment="Bottom"></Ellipse>
                    <Ellipse Width="10" Height="10" Stroke="Black" Fill="Black" VerticalAlignment="Bottom" Margin="0,0,0,5"></Ellipse>
                    <Line Y1="0" Y2="10" Stroke="Black" StrokeThickness="1" HorizontalAlignment="Center"></Line>
                </Grid>
            </Border>
    </Grid>
</UserControl>
